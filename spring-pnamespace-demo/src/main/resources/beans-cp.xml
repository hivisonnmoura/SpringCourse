<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="myorg" class="com.hivison.study.java.spring.domain.Organization"
	p:companyName="BMW" p:yearOfIncorporation="1929" p:address-ref="address">
	</bean>
	<bean id="address" class="com.hivison.study.java.spring.domain.Address"
	p:street="15 Blue Berry Lane" p:city="Los Angeles" p:zipCode="132465">
	</bean>
</beans>
